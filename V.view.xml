<mvc:View
	controllerName="sap.tnt.sample.ToolHeaderIconTabHeader.V"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:tnt="sap.tnt"
	height="100%">
	<tnt:ToolHeader>
		<Button icon="sap-icon://home" type="Transparent" press="onHomePress">
			<layoutData>
				<OverflowToolbarLayoutData priority="NeverOverflow" />
			</layoutData>
		</Button>
		<ToolbarSpacer />
		<!-- <core:Icon src="sap-icon://activate">
		</core:Icon> -->
		<Text text="Sunil B N" wrapping="false">
			<layoutData>
				<OverflowToolbarLayoutData priority="NeverOverflow" />
			</layoutData>
		</Text>
		<IconTabHeader id="iconTabHeader"
			selectedKey="invalidKey"
			select="onSelectTab"
			showOverflowSelectList="true"
			backgroundDesign="Transparent"
			mode="Inline">
			<layoutData>
				<OverflowToolbarLayoutData
					priority="NeverOverflow"
					shrinkable="true" />
			</layoutData>
			<items>
				<IconTabFilter text="About">
				</IconTabFilter>
				<IconTabFilter text="Skills">
				</IconTabFilter>
				<IconTabFilter text="Portfolio">
				</IconTabFilter>
				<IconTabFilter text="Achievements">
				</IconTabFilter>
			</items>
		</IconTabHeader>
<!--
		<ToolbarSpacer>
			<layoutData>
				<OverflowToolbarLayoutData priority="Disappear" minWidth="16px" />
			</layoutData>
		</ToolbarSpacer> -->
		<Button icon="sap-icon://business-card" type="Transparent">
			<layoutData>
				<OverflowToolbarLayoutData priority="NeverOverflow" />
			</layoutData>
		</Button>

	</tnt:ToolHeader>
	<Panel>
		<Label
			id="labelId"
			text="Under Developement"/>
	</Panel>
</mvc:View>
