<mvc:View controllerName="sap.m.sample.GenericTileAsFeedTile.Page"
	xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Feed Tile that shows updates of the last feeds given to a specific topic:"
		frameType="TwoByOne" press="press">
		<TileContent footer="New Notifications">
			<FeedContent contentText="@@notify Great outcome of the Presentation today. New functionality well received."
				subheader="About 1 minute ago in Computer Market" value="352" />
		</TileContent>
	</GenericTile>

	<SlideTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout">
		<tiles>
			<GenericTile
				backgroundImage="test-resources/sap/m/demokit/sample/GenericTileAsFeedTile/images/NewsImage1.png"
				frameType="TwoByOne" press="press">
				<TileContent footer="August 21, 2016">
					<NewsContent
						contentText="This in under construction"
						subheader="As of Today" />
				</TileContent>
			</GenericTile>
			<GenericTile
				backgroundImage="test-resources/sap/m/demokit/sample/GenericTileAsFeedTile/images/NewsImage2.png"
				frameType="TwoByOne" press="press">
				<TileContent footer="August 21, 2016">
					<NewsContent
						contentText="I will build this soon"
						subheader="As of yesterday" />
				</TileContent>
			</GenericTile>
		</tiles>
	</SlideTile>
</mvc:View>
